# booleans
true = Pack{1,0};
false = Pack{2,0};
if c t f = case c of	<1> -> t, <2> -> f;

# lists
cons h t = Pack{3,2} h t;
nil = Pack{4,0};
hd a = case a of <3> h t -> h;
tl a = case a of <3> h t -> t;

# misc
I x = x;
K x y = x;
K1 x y = y;
S f g x = f x (g x);
compose f g x = f (g x);
twice f = compose f f